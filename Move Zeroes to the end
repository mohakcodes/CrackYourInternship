class Solution {
public:
    void moveZeroes(vector<int>& arr)
    {
        int i=0; int j=0;
        int n=arr.size();
        
        while(j<n)
        {
            if(arr[j] == 0)
            { j++; }
            
            else
            {
                swap(arr[i] , arr[j]);
                i++; j++;
            }
        }
    }
};
